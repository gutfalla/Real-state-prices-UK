#github
  #create a new repository

echo "# test" >> README.md
git init
git add README.md
git commit -m "first commit"
git branch -M main
git remote add origin git@github.com:gutfalla/test.git
git push -u origin main

  #push an existing repository

git remote add origin git@github.com:gutfalla/test.git
git branch -M main
git push -u origin main
